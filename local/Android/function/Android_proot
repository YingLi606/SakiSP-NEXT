		log "é€€å‡º${os_name}"
		;;
	esac
}
rm_proot() {
	list_dir ${HOME}/.termux/SakiSP-NEXT/proot
	case $user_choice in
	0)
		echo -e "${RED}QUIT${RES}"
		log "é€€å‡º"
		;;
	*)
		if [ ! $? = 0 ]; then
			exit
		fi
		os_name=${list_items[$((user_choice - 1))]}
		num1=$((RANDOM % 100))
		num2=$((RANDOM % 100))
		log "num1=$num1 num2=$num2"
		# è¯»å–ç”¨æˆ·è¾“å…¥
		read -e -p "${num1}+${num2}=?" user_sum
		log "user_sum=$user_sum"
		# è®¡ç®—æ­£ç¡®çš„å’Œ
		correct_sum=$(($num1 + $num2))
		# æ£€æŸ¥ç”¨æˆ·è¾“å…¥æ˜¯å¦æ­£ç¡®
		if [ $user_sum -eq $correct_sum ]; then
			echo -e "âœ… éªŒè¯å®Œæˆï¼æ‚¨å·²è¯æ˜ä½ æ˜¯äººç±»ï¼Œç»§ç»­æ“ä½œ"
			sleep 2
			chmod 777 -R "${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/"
			rm -rfv "${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/"
			echo -e "åˆ é™¤å®Œæˆ,æ­£åœ¨é€€å‡º"
			sleep 2
			clear
		else
			echo -e "âŒ é”™è¯¯ï¼ä½ è¾“å…¥çš„ç­”æ¡ˆä¸æ­£ç¡®ï¼Œæœ¬æ¬¡éªŒè¯ä¸é€šè¿‡"
			sleep 2
			clear
			exit 1
		fi
		;;
	esac
}
