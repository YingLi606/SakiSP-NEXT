	sleep 0.1
	echo -e "\e[92m2. âšªï¸ Forkyï¼ˆDebian 13 æ»šåŠ¨æ›´æ–°ç‰ˆï¼‰\e[0m"	
	sleep 0.1
	echo -e "\e[93m3. ğŸŸ¡ Bookwormï¼ˆDebian 12ï¼‰\e[0m"
