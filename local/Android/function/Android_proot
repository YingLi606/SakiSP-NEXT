rm_proot() {
	list_dir ${HOME}/.termux/SakiSP-NEXT/proot
	case $user_choice in
	0)
		echo -e "${RED}QUIT${RES}"
		log "é€€å‡º"
		;;
	*)
		if [ ! $? = 0 ]; then
			exit
		fi
		os_name=${list_items[$((user_choice - 1))]}
		num1=$((RANDOM % 100))
		num2=$((RANDOM % 100))
		log "num1=$num1 num2=$num2"
		read -e -p "ğŸ§® äººç±»éªŒè¯ï¼š${num1}+${num2}=?" user_sum
		log "user_sum=$user_sum"
		correct_sum=$(($num1 + $num2))
		if [ $user_sum -eq $correct_sum ]; then
			echo -e "\e[92mâœ… éªŒè¯å®Œæˆï¼æ‚¨å·²è¯æ˜æ˜¯äººç±»ï¼Œç»§ç»­æ“ä½œ \e[0m"
			sleep 2
			chmod 777 -R "${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/"
			rm -rfv "${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/"
			echo -e "\e[92mâœ… åˆ é™¤å®Œæˆï¼Œæ­£åœ¨é€€å‡º \e[0m"
			sleep 2
			clear
		else
			echo -e "\e[91mâŒ é”™è¯¯ï¼è¾“å…¥ç­”æ¡ˆä¸æ­£ç¡®ï¼ŒéªŒè¯ä¸é€šè¿‡ \e[0m"
			sleep 2
			clear
			exit 1
		fi
		;;
	esac
}