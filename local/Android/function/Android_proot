install_proot() {
	case "$(uname -m)" in
	aarch64)
		archurl="arm64"
		;;
	armv7l)
		archurl="armhf"
		;;
	x86_64)
		archurl="amd64"
		;;
	*)
		echo -e "\e[91m âŒ é”™è¯¯ï¼šæœªçŸ¥æ¡†æ¶ \e[0m"
		exit
		;;
	esac
	mkdir -p "${HOME}/.termux/SakiSP-NEXT/proot"
	Proot_os_download_source="https://sgp1lxdmirror01.do.letsbuildthe.cloud/images"
	clear
	echo -e "\n\e[92mğŸ‰ \e[1mã€é€‰æ‹©å®¹å™¨ç³»ç»Ÿç±»å‹ã€‘\e[0m"
	echo -e "\e[91m1. ğŸ Ubuntu\e[0m"
	echo -e "\e[93m2. ğŸ¥ Debian\e[0m"
	echo -e "\e[95m3. ğŸ¥‘ Kali\e[0m"
	echo -e "\e[96m4. ğŸ Rockylinux\e[0m"
	echo -e "\e[92m5. ğŸ¥ CentOS\e[0m"
	echo -e "\e[91m0. ğŸŒš é€€å‡º\e[0m"
	read -p "è¯·è¾“å…¥å¯¹åº”æ•°å­—é€‰æ‹©ï¼š" open_proot_os_list
	case ${open_proot_os_list} in
	1)
		open_ubuntu_os_list
		log "å®‰è£…ubuntuç³»åˆ—"
		;;
	2)
		open_debian_os_list
		log "debianç³»åˆ—"
		;;
	3)
		open_kali_os_list
		log "å®‰è£…kaliç³»åˆ—"
		;;
	4)
		open_rockylinux_os_list
		log "å®‰è£…rockylinuxç³»åˆ—"
		;;
	5)
		open_centos_os_list
		log "å®‰è£…centosç³»åˆ—"
		;;
	*)
		log "æœªé€‰æ‹©é€€å‡º"
		clear
		exit 1
		;;
	esac
	log "os_name: ${os_name}"
	log "Download_proot_NAME: ${Download_proot_NAME}"
	log "Download_proot_url: ${Download_proot_url}"
	wget_proot_url(){
		while [ $counter -lt $Maximum_number_of_attempts ]; do
			log "æœ€å¤§å¾ªç¯Maximum_number_of_attempts: $Maximum_number_of_attempts"
			log "å½“å‰å¾ªç¯counter= $counter"
			if wget -q --show-progress "${Download_proot_url}" -O "$download_file"; then
				echo -e "\e[92m âœ… æˆåŠŸï¼šæ–‡ä»¶å­˜å‚¨åœ¨ ${download_file} \e[0m"
				log "ä¸‹è½½æˆåŠŸï¼š$download_file"
				break
			else
				echo -e "\e[93m âš ï¸  è­¦å‘Šï¼šè¯·æ£€æŸ¥æ‚¨çš„ç½‘ç»œç¯å¢ƒ \e[0m"
				log "ä¸‹è½½å¤±è´¥ï¼š$download_file"
				if [ $counter -eq 0 ]; then
					Download_proot_url="https://dl.gancmcs.top/${Download_proot_url}"
					log "å°è¯•ä½¿ç”¨å¤‡ç”¨ URL ä¸‹è½½ï¼Œæ›´æ–° Download_proot_url ä¸º \"$Download_proot_url\""
				fi
				if [ $counter -eq $((Maximum_number_of_attempts - 1)) ]; then
					echo -e "\e[91m âŒ é”™è¯¯ï¼šå¤šæ¬¡ä¸‹è½½å¤±è´¥ï¼Œé€€å‡ºç¨‹åº \e[0m"
					exit 1
				fi
			fi
			((counter++))
		done
	}
	download_file="${HOME}/SakiSP-NEXT/download/${os_name}.tar.xz"
	if [ -f "$download_file" ] && [ $(du -b "$download_file" | cut -f1) -lt 41943040 ]; then
		echo -e "\e[93m âš ï¸  è­¦å‘Šï¼š${download_file} å¯èƒ½å·²ç»æŸå \e[0m"
		log "\"${HOME}/SakiSP-NEXT/download/${os_name}.tar.xz\"æ–‡ä»¶æŸåï¼Œé‡æ–°ä¸‹è½½"
		log "ä¸‹è½½url:\"${Download_proot_url}\" ä¿å­˜åˆ° \"$download_file\""
		counter=0
		Maximum_number_of_attempts=2
		wget_proot_url
	elif [ -f "$download_file" ]; then
		log "\"${HOME}/SakiSP-NEXT/download/${os_name}.tar.xz\"å­˜åœ¨ä¸”å¤§äº40MBB"
	else
		log "\"${HOME}/SakiSP-NEXT/download/${os_name}.tar.xz\"ä¸å­˜åœ¨"
		log "ä¸‹è½½url:\"${Download_proot_url}\" ä¿å­˜åˆ° \"$download_file\""
		counter=0
		Maximum_number_of_attempts=2
		wget_proot_url
	fi
	echo -e "\e[94mæç¤ºï¼šæ‚¨å¯ä»¥ä½¿ç”¨ rm -rfv $download_file æ¥èŠ‚çœå­˜å‚¨ç©ºé—´ \e[0m"
	unset LD_PRELOAD
	mkdir -p ${HOME}/.termux/SakiSP-NEXT/proot/${os_name}
	log "è§£å‹\"${HOME}/SakiSP-NEXT/download/${os_name}.tar.xz\""
	proot --link2symlink tar --overwrite -xJf ${HOME}/SakiSP-NEXT/download/${os_name}.tar.xz -C ${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/ --exclude='dev' || :
	chmod 777 "${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/root/"
	chmod 777 "${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/"
	echo -e "\e[92mè¿›åº¦ï¼šå¤åˆ¶è„šæœ¬åˆ°å®¹å™¨ \e[0m"
	log "å¤åˆ¶è„šæœ¬åˆ°å®¹å™¨"
	mkdir -p ${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/root/SakiSP-NEXT
	mkdir -p ${HOME}/.termux/SakiSP-NEXT/.é‡è¦æ–‡ä»¶å¤¹/sys/.empty
	mkdir -p ${HOME}/.termux/SakiSP-NEXT/.é‡è¦æ–‡ä»¶å¤¹/dev/shm
	cp -r ${HOME}/SakiSP-NEXT/config ${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/root/SakiSP-NEXT
	cp -r ${HOME}/SakiSP-NEXT/function ${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/root/SakiSP-NEXT
	cp -r ${HOME}/SakiSP-NEXT/local ${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/root/SakiSP-NEXT
	cp ${HOME}/SakiSP-NEXT/sakispnext.sh ${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/root/SakiSP-NEXT
	cp ${HOME}/SakiSP-NEXT/git_push.sh ${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/root/SakiSP-NEXT
	echo -e "\e[92mè¿›åº¦ï¼šå¤åˆ¶ä¼˜åŒ–è„šæœ¬åˆ°å®¹å™¨ \e[0m"
	cp ${HOME}/SakiSP-NEXT/function/proot_proc/.* ${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/
	cp ${HOME}/SakiSP-NEXT/function/proot_optimization ${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/root/ä¼˜åŒ–.sh
	proot \
		--link2symlink -0 \
		--rootfs=${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/ \
		--bind=/dev \
		--bind=/sys \
		--bind=/proc \
		--bind="${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.loadavg:/proc/loadavg" \
		--bind="${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.version:/proc/version" \
		--bind="${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.uptime:/proc/uptime" \
		--bind="${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.stat:/proc/stat" \
		--bind="${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.vmstat:/proc/vmstat" \
		--bind="${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.sysctl_entry_cap_last_cap:/proc/sysctl_entry_cap_last_cap" \
		--kernel-release="5.15.0" \
		-w /root /usr/bin/env \
		-i HOME=/root \
		PATH=/usr/local/sbin:/usr/local/bin:/bin:/usr/bin:/sbin:/usr/sbin:/usr/games:/usr/local/games \
		TERM=xterm-256color \
		LANG=zh_CN.UTF-8 \
		LC_CTYPE="zh_CN.UTF-8" \
		LC_NUMERIC="zh_CN.UTF-8" \
		LC_TIME="zh_CN.UTF-8" \
		LC_COLLATE="zh_CN.UTF-8" \
		LC_MONETARY="zh_CN.UTF-8" \
		LC_MESSAGES="zh_CN.UTF-8" \
		LC_PAPER="zh_CN.UTF-8" \
		LC_NAME="zh_CN.UTF-8" \
		LC_ADDRESS="zh_CN.UTF-8" \
		LC_TELEPHONE="zh_CN.UTF-8" \
		LC_MEASUREMENT="zh_CN.UTF-8" \
		LC_IDENTIFICATION="zh_CN.UTF-8" \
		/bin/bash ä¼˜åŒ–.sh
	log "ä¼˜åŒ–è„šæœ¬æ‰§è¡ŒæˆåŠŸ"
	log "é€€å‡ºå®¹å™¨"
}
start_proot() {
	list_dir ${HOME}/.termux/SakiSP-NEXT/proot
	case $user_choice in
	0)
		echo -e "${RED}QUIT${RES}"
		log "é€€å‡º"
		;;
	*)
		if [ ! $? = 0 ]; then
			exit
		fi
		os_name=${list_items[$((user_choice - 1))]}
		log "è¿›å…¥å®¹å™¨${os_name}"
		unset LD_PRELOAD
		proot \
			--bind=/data/data/com.termux/files/usr \
			--bind=/vendor \
			--bind=/system \
			--bind=/system/product \
			--bind=/odm \
			--bind=/apex \
			--bind=/data/data/com.termux/cache \
			--bind=/data/dalvik-cache \
			--bind=/data/app \
			--bind=/proc:/proc \
			--bind=${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.sysctl_inotify_max_user_watches:/proc/sys/fs/inotify/max_user_watches \
			--bind=${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.sysctl_entry_cap_last_cap:/proc/sys/kernel/cap_last_cap \
			--bind=${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.vmstat:/proc/vmstat \
			--bind=${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.version:/proc/version \
			--bind=${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.uptime:/proc/uptime \
			--bind=${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.stat:/proc/stat \
			--bind=${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/proc/.loadavg:/proc/loadavg \
			--bind=${HOME}/.termux/SakiSP-NEXT/.é‡è¦æ–‡ä»¶å¤¹/sys/.empty:/sys/fs/selinux \
			--bind=${HOME}/.termux/SakiSP-NEXT/.é‡è¦æ–‡ä»¶å¤¹/dev/shm:/dev/shm \
			--bind=/sys \
			--bind=/proc/self/fd/2:/dev/stderr \
			--bind=/proc/self/fd/1:/dev/stdout \
			--bind=/proc/self/fd/0:/dev/stdin \
			--bind=/proc/self/fd:/dev/fd \
			--bind=/proc \
			--bind=/dev/urandom:/dev/random \
			--bind=/dev \
			-L \
			--kernel-release=6.2.1-PRoot-Distro \
			--sysvipc \
			--link2symlink \
			--kill-on-exit \
			--cwd=/root \
			--change-id=0:0 \
			--rootfs=${HOME}/.termux/SakiSP-NEXT/proot/${os_name}/ \
			/usr/bin/env -i \
			PATH=/usr/local/sbin:/usr/local/bin:/bin:/usr/bin:/sbin:/usr/sbin:/usr/games:/usr/local/games \
			TERM=xterm-256color \
			system_os_type=proot \
			system_os_type2=${os_name} \
			LANG=zh_CN.UTF-8 \
			LC_CTYPE="zh_CN.UTF-8" \
			LC_NUMERIC="zh_CN.UTF-8" \
			LC_TIME="zh_CN.UTF-8" \
			LC_COLLATE="zh_CN.UTF-8" \
			LC_MONETARY="zh_CN.UTF-8" \
			LC_MESSAGES="zh_CN.UTF-8" \
			LC_PAPER="zh_CN.UTF-8" \
			LC_NAME="zh_CN.UTF-8" \
			LC_ADDRESS="zh_CN.UTF-8" \
			LC_TELEPHONE="zh_CN.UTF-8" \
			LC_MEASUREMENT="zh_CN.UTF-8" \
			LC_IDENTIFICATION="zh_CN.UTF-8" \
			ANDROID_DATA=/data \
			ANDROID_ROOT=/system \
			ANDROID_RUNTIME_ROOT=/apex/com.android.runtime \
